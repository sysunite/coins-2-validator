<?xml version="1.0" encoding="UTF-8"?>
<debug>
  <valid>${valid?c}</valid>
  <steps>
<#list steps?keys as key>
    <step>
      <name>${key}</name>
      <valid>${steps[key]?c}</valid>
    </step>
</#list>
  </steps>
  <variables>
<#list .data_model?keys as key>
    <variable>
      <key>${key}</key>
    </variable>
</#list>
  </variables>
</debug>